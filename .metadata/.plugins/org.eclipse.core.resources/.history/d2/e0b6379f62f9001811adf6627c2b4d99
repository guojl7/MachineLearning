#-*- coding:utf-8 -*-
from numpy import *
import SVD
import matplotlib.pyplot as plt

def loadExData():
    return[[4, 4, 0, 2, 2],
           [4, 0, 0, 3, 3],
           [4, 0, 0, 1, 1],
           [1, 1, 1, 2, 0],
           [2, 2, 2, 0, 0],
           [1, 1, 1, 0, 0],
           [5, 5, 5, 0, 0]]

if __name__ == '__main__':
    myMat = mat(loadExData())
    print(SVD.recommend(myMat, 2, simMeas = SVD.ecludSim))
    
    print("SVD: %r" % SVD.recommend(myMat, 2, estMethod = SVD.svdEst ,simMeas = SVD.pearsSim))
